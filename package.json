{
  "name": "@jonz94/discord-bot-ufo",
  "version": "2.1.0",
  "description": "A Discord BOT named 機器幽浮 (robot UFO)",
  "license": "0BSD",
  "author": {
    "name": "jonz94",
    "email": "jody16888@gmail.com"
  },
  "type": "module",
  "scripts": {
    "build": "tsup",
    "db": "drizzle-kit",
    "db:generate": "drizzle-kit generate:sqlite",
    "db:migrate": "tsx migrate.mts",
    "db:studio": "drizzle-kit studio",
    "dev": "cross-env NODE_ENV=DEV tsx watch src/main.mts",
    "fmt": "prettier --write .",
    "release": "commit-and-tag-version",
    "setup": "tsx scripts/initialize-env-files.mts",
    "start": "node dist/main.js",
    "start:dev": "cross-env NODE_ENV=DEV node dist/main.js",
    "test": "vitest"
  },
  "dependencies": {
    "better-sqlite3": "^9.4.3",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.30.2"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.9",
    "commit-and-tag-version": "^12.2.0",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.20.14",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-packagejson": "^2.4.12",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2",
    "vitest": "^1.4.0"
  },
  "engines": {
    "node": ">=20.11.1",
    "pnpm": ">=8.15"
  }
}
